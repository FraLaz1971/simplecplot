#to compile all on linux
./configure
make -f Makefile.lnx
#to compile all on MSwin+msys+mingw
./configure
mingw32-make -f Makefile.mingw
#to run insults c exe on linux
cd src
./insults
#to run insults c exe on ms win cmd
cd src
.\insults.exe
#to run insults c exe on ms win msys2/mingw
cd src
./insults.exe
#to run insults c exe on linux
cd src
./integers
#to run insults c exe on ms win cmd
cd src
.\integer.exe
#to run insults c exe on ms win msys2/mingw
cd src
./integers.exe
#to run insults.sh bash script
cd script/bash
bash insults.sh
#to run integers.sh bash script
cd script/bash
bash integers.sh
#to run python programs modify 
#the actual command to the use
#of python3 or python2
